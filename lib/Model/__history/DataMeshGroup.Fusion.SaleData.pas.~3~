unit DataMeshGroup.Fusion.SaleData;

interface

type
  TSaleData = class
  private
    FOperatorID: string;
  public
    property OperatorID: string read FOperatorID write FOperatorID;
    property string OperatorLanguage { get; set; }
    property string ShiftNumber { get; set; }
    property string CustomerLanguage { get; set; }
    property TransactionIdentification SaleTransactionID { get; set; }
    property string SaleReferenceID { get; set; }
    property SaleTerminalData SaleTerminalData { get; set; }
    property TokenRequestedType? TokenRequestedType { get; set; }
    property string CustomerOrderID { get; set; }
    property List<CustomerOrderReq> CustomerOrderReq { get; set; }
    property string SaleToPOIData { get; set; }
    property string SaleToAcquirerData { get; set; }
    //property SaleToIssuerData SaleToIssuerData { get; set; }
    //property SponsoredMerchant SponsoredMerchant { get; set; }
  end;

